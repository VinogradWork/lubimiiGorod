						<tr class="d-formats__table-row">
							<td>Вертикальный</td><td>А4</td><td>200*260</td><td>PDF, TIFF, PSD, CDR, EPS</td>
						</tr>
						<tr class="d-formats__table-row">
							<td>Горизонтальный</td><td>А4</td><td>130*403</td><td>PDF, TIFF, PSD, CDR, EPS</td>
						</tr>
						<tr class="d-formats__table-row">
							<td>Вертикальный</td><td>А5</td><td>130*200</td><td>PDF, TIFF, PSD, CDR, EPS</td>
						</tr>
						<tr class="d-formats__table-row">
							<td>Горизонтальный</td><td>А6</td><td>98*130</td><td>PDF, TIFF, PSD, CDR, EPS</td>
						</tr>
						<tr class="d-formats__table-row">
							<td colspan="2">Полоса</td><td>37*260</td><td>PDF, TIFF, PSD, CDR, EPS</td>
						</tr>
						</tbody>
						
					</table>
				</div>
				
				</div>
			</div>
		</div>
	</div>
</div>


